msgstr "Den teckenkollation som sattes när användardatabasen skapades."

#: bb-admin/includes/class.bb-install.php:859
#: bb-admin/options-wordpress.php:151
msgid "User database \"user\" table"
msgstr "Användardatabasens \"user\"-tabell"

#: bb-admin/includes/class.bb-install.php:860
#: bb-admin/includes/class.bb-install.php:866
#: bb-admin/options-wordpress.php:152
#: bb-admin/options-wordpress.php:156
msgid "The complete table name, including any prefix."
msgstr "Hela tabellnamnet, inklusive eventuella prefix."

#: bb-admin/includes/class.bb-install.php:865
#: bb-admin/options-wordpress.php:155
msgid "User database \"user meta\" table"
msgstr "Användardatabasens \"user meta\"-tabell"

#: bb-admin/includes/class.bb-install.php:876
msgid "Go to step 3"
msgstr "Gå till steg 3"

#: bb-admin/includes/class.bb-install.php:885
msgid "Site name"
msgstr "Webbplatsens namn"

#: bb-admin/includes/class.bb-install.php:886
